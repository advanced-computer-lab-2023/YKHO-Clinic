<!DOCTYPE html>
<html>

<head>
    <title>register</title>
</head>

<body>
    <h3>register</h3>
    <form method="post" action="/patient/createPatient">
        <label for="username">username</label><br>
        <input type="text" id="username" name="username"><br>
        <label for="password">password</label><br>
        <input type="password" id="password" name="password"><br>
        <label for="name">name</label><br>
        <input type="text" id="name" name="name"><br>
        <label for="DOB">DOB</label><br>
        <input type="date" id="DOB" name="DOB"><br>
        <input type="radio" id="male" name="gender" value="male">
        <label for="male">male</label>
        <br>
        <input type="radio" id="female" name="gender" value="female">
        <label for="female">female</label>
        <br>
        <label for="email">email</label><br>
        <input type="email" id="email" name="email"><br>
        <label for="mobile">mobile</label><br>
        <!--type-->
        <input type ="number" id="mobile" name="mobile"><br>
        <h4>emergency contact</h4>
        <label for="emergencyName">name</label><br>
        <input type="text" id="emergencyName" name="emergencyName"><br>
        <!--type-->
        <label for="emergencyMobile">mobile</label><br>
        <input type ="number" id="emergencyMobile" name="emergencyMobile"><br>
        <br>
        <input type="submit" value="register">
        <%= message %>
    </form>
</body>

</html>